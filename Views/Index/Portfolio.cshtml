	})
	$('.FlappyBirdImgArrow').click(function (event) {

		OpenFlappyBird();

	})
	$('.FlappyBirdClose').click(function (event) {

		CloseFlappyBird();

	})
	$('.ResearchImg').click(function (event) {

		OpenResearch();

	})
	$('.ResearchImgArrow').click(function (event) {

		OpenResearch();

	})
	$('.ResearchClose').click(function (event) {

		CloseResearch();

	})
	
	$('.SlideIndex1').click(function (event) {
		//3 if statements to determine method call
        if (ProjectTypeIndex == 0) {
            showDivs(slideIndex = 1);
        }
        else if (ProjectTypeIndex == 1) {
            showGameDivs(slideIndex = 1);
        }
        else if (ProjectTypeIndex == 2) {
			showResearchDivs(slideIndex = 1);
		}
		
		$('.SlideIndex1').css("background-color", "#b59a5a");
		$('.SlideIndex2').css("background-color", "white");
		$('.SlideIndex3').css("background-color", "white");

	})

	$('.SlideIndex2').click(function (event) {
        if (ProjectTypeIndex == 0) {
            showDivs(slideIndex = 2);
        }
        else if (ProjectTypeIndex == 1) {
            showGameDivs(slideIndex = 2);
        }
        else if (ProjectTypeIndex == 2) {
			showResearchDivs(slideIndex = 2);
		}
        $('.SlideIndex2').css("background-color", "#b59a5a");
		$('.SlideIndex1').css("background-color", "white");
		$('.SlideIndex3').css("background-color", "white");
	})

	$('.SlideIndex3').click(function (event) {
        if (ProjectTypeIndex == 0) {
            showDivs(slideIndex = 3);
        }
        else if (ProjectTypeIndex == 1) {
            showGameDivs(slideIndex = 3);
        }
        else if (ProjectTypeIndex == 2) {
			showResearchDivs(slideIndex = 3);
		}
        $('.SlideIndex3').css("background-color", "#b59a5a");
		$('.SlideIndex1').css("background-color", "white");
		$('.SlideIndex2').css("background-color", "white");
	})

	$('.prev').click(function (event) {
        plusDivs(-1);
	})

	$('.next').click(function (event) {
        plusDivs(1);
	})

	$(function() {
		$("#selectProjectType").on("change",function() {
			const projectType = this.value;
            if (projectType == "") {
                return;
            }
            else if (projectType == "WebApplications") {
				$('.SlideIndex1').css("background-color", "#b59a5a");
				$('.SlideIndex2').css("background-color", "white");
				$('.SlideIndex3').css("background-color", "white");
				showDivs(slideIndex = 1);
            }
            else if (projectType == "Games") {
				$('.SlideIndex1').css("background-color", "#b59a5a");
				$('.SlideIndex2').css("background-color", "white");
				$('.SlideIndex3').css("background-color", "white");
				showGameDivs(slideIndex = 1);
			}
			else if (projectType == "Research") {
				$('.SlideIndex1').css("background-color", "#b59a5a");
				$('.SlideIndex2').css("background-color", "white");
				$('.SlideIndex3').css("background-color", "white");
				showResearchDivs(slideIndex = 1);
			}
		}); 
	});

	function ShowWebSlideIndexes() {
        $('.SlideIndex2').css("display", "inline-block");
        $('.SlideIndex3').css("display", "inline-block");
	}

	function ShowGameSlideIndexes() {
        $('.SlideIndex2').css("display", "none");
        $('.SlideIndex3').css("display", "none");
		$('.prev').css("display", "none");
		$('.next').css("display", "none");
	}

	function ShowResearchSlideIndexes() {
        $('.SlideIndex2').css("display", "none");
        $('.SlideIndex3').css("display", "none");
		$('.prev').css("display", "none");
		$('.next').css("display", "none");
	}

	function CloseDialogs() {
        ClosePowerPuzzle();
        ClosePortfolio1();
        CloseAuthy();
        ClosePortfolio2();
        CloseScrum();
        CloseLegendsBlade();
        CloseFlappyBird();
        CloseResearch();
	}

    function HideGames() {

        var i = 0;
		var WebAppSlides3 = $('.WebAppSlides3');
		var WebAppSlides4 = $('.WebAppSlides4');

		for (i = 0; i < WebAppSlides3.length; i++) {
            $('.WebAppSlides3.Shown').addClass("None");
			$('.WebAppSlides3.Shown').removeClass("Shown");
		}

		for (i = 0; i < WebAppSlides4.length; i++) {
            $('.WebAppSlides4.Shown').addClass("None");
			$('.WebAppSlides4.Shown').removeClass("Shown");
		}
	}

    function HideResearch() {

        var i = 0;
		var WebAppSlides5 = $('.WebAppSlides5');
		var WebAppSlides6 = $('.WebAppSlides6');

		for (i = 0; i < WebAppSlides5.length; i++) {
            $('.WebAppSlides5.Shown').addClass("None");
			$('.WebAppSlides5.Shown').removeClass("Shown");
		}

		for (i = 0; i < WebAppSlides6.length; i++) {
            $('.WebAppSlides6.Shown').addClass("None");
			$('.WebAppSlides6.Shown').removeClass("Shown");
		}
	}

	function HideWebApps() {

		var i = 0;
		var WebAppSlides1 = $('.WebAppSlides1');
		var WebAppSlides2 = $('.WebAppSlides2');

		for (i = 0; i < WebAppSlides1.length; i++) {
            $('.WebAppSlides1.Shown').addClass("None");
			$('.WebAppSlides1.Shown').removeClass("Shown");
		}

		for (i = 0; i < WebAppSlides2.length; i++) {
            $('.WebAppSlides2.Shown').addClass("None");
			$('.WebAppSlides2.Shown').removeClass("Shown");
		}
	}

	function OpenLegendsBlade() {
		//need new legends blade string
        result = LegendsBladeString.split('\n').map(e => `<p>${e}</p>`);

		$('.LegendsBladeImgFlexContainer').hide();
		$('.LegendsBladeContainer').css("display", "block");
		$('.LegendsBladeClose').css("display", "inline-block");
        $('.LegendsBladeExperience')[0].innerHTML = result.join('');
		$('.LegendsBladeExperience')[0].innerHTML += "<a href='"+"https://github.com/gcobb1/ALegendsBlade"+"'>View Code</a>";
		$('.LegendsBladeExperience')[0].innerHTML += "<a href='"+"https://gavincobb.itch.io/a-legends-blade"+"'>Download Game</a>";
	}

	function CloseLegendsBlade() {

        $('.LegendsBladeContainer').hide();
		$('.LegendsBladeImgFlexContainer').css("display", "flex");
		$('.LegendsBladeClose').hide();
	}

