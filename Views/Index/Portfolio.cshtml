	function plusDivs(n) {
		if (ProjectTypeIndex == 0) {
            showDivs(slideIndex += n);
        }
        else if (ProjectTypeIndex == 1) {
            showGameDivs(slideIndex += n);
        }
        else if (ProjectTypeIndex == 2) {
			showResearchDivs(slideIndex += n);
		}
        if (slideIndex == 1) {
			$('.SlideIndex1').css("background-color", "#b59a5a");
			$('.SlideIndex2').css("background-color", "white");
			$('.SlideIndex3').css("background-color", "white");
        }
        else if(slideIndex == 2){
			$('.SlideIndex2').css("background-color", "#b59a5a");
			$('.SlideIndex1').css("background-color", "white");
			$('.SlideIndex3').css("background-color", "white");
        }
		else if(slideIndex == 3){
			$('.SlideIndex3').css("background-color", "#b59a5a");
			$('.SlideIndex1').css("background-color", "white");
			$('.SlideIndex2').css("background-color", "white");
		}
	}
	
	function showDivs(n) {
		
		CloseDialogs();
        HideWebApps();
        HideGames();
        HideResearch();
        ShowWebSlideIndexes();

		var i;
		var secondIndex = 0;
		ProjectTypeIndex = 0;

		$('.webapp-grid').eq(1).show();

		var WebAppSlides1 = $('.WebAppSlides1');
		var WebAppSlides2 = $('.WebAppSlides2');

        if (n == WebAppSlides1.length) {
            $('.next').css("display", "none");

            if (n == WebAppSlides2.length) {
                secondIndex = n - 1;
                $('.WebAppSlides2.None').eq(secondIndex).addClass("Shown"); //only do this to the child with the secondIndex
                $('.WebAppSlides2.None').eq(secondIndex).removeClass("None"); //only do this to the child with the secondIndex
				$('.webapp-label-2')[0].innerHTML = Titles[n + 2];
				$('.webapp-button2 a').attr("href", Links[n + 2]);
            }
            else {
                $('.webapp-grid').eq(1).hide();
			}
        }
        else {
			$('.next').css("display", "inline-block");

			secondIndex = n - 1;
			$('.WebAppSlides2.None').eq(secondIndex).addClass("Shown");
			$('.WebAppSlides2.None').eq(secondIndex).removeClass("None");
			$('.webapp-label-2')[0].innerHTML = Titles[n + 2];
			$('.webapp-button2 a').attr("href", Links[n + 2]);
		}

        if (n == 1) {
            $('.prev').css("display", "none");
        }
        else {
			$('.prev').css("display", "inline-block");
		}
		
		$('.WebAppSlides1.None').eq(n-1).addClass("Shown");
		$('.WebAppSlides1.None').eq(n-1).removeClass("None");
        $('.webapp-label-1')[0].innerHTML = Titles[n - 1];
